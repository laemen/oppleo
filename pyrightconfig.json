{
    "include": [
      "src"
    ],
    
    "exclude": [
      "**/node_modules",
      "**/__pycache__",
      "src/experimental",
      "src/typestubs"
    ],
  
    "ignore": [
      "src/oldstuff"
    ],
  
    "stubPath": "src/stubs",
    "venv": "env367",
  
    "reportMissingImports": true,
    "reportMissingTypeStubs": false,
  
    "pythonVersion": "3.14",
    "pythonPlatform": "Linux",
  
    "executionEnvironments": [
      {
        "root": "src/web",
        "pythonVersion": "3.14",
        "pythonPlatform": "Windows",
        "extraPaths": [
          "src/service_libs"
        ]
      },
      {
        "root": "src/sdk",
        "pythonVersion": "3.14",
        "extraPaths": [
          "src/backend"
        ]
      },
      {
        "root": "src/tests",
        "extraPaths": [
          "src/tests/e2e",
          "src/sdk"
        ]
      },
      {
        "root": "src"
      }
    ]
  }